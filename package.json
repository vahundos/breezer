{
  "name": "breezer",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/vahundos/breezer.git",
  "scripts": {
    "dev-server": "webpack-dev-server --config webpack.dev.js",
    "test": "jest --verbose",
    "build": "webpack --config webpack.prod.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "modulePaths": [
      "<rootDir>/src/main/js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/main/js/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "axios": "^0.27.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.4",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@vue/test-utils": "1.3.5",
    "babel-bridge": "1.12.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.5",
    "css-loader": "3.6.0",
    "deepmerge": "4.2.2",
    "fibers": "5.0.3",
    "jest": "25.5.4",
    "html-webpack-plugin": "4.5.2",
    "jest": "26.6.3",
    "nock": "12.0.3",
    "sass": "1.53.0",
    "sass-loader": "8.0.2",
    "vue-jest": "3.0.7",
    "vue-loader": "15.9.8",
    "vue-style-loader": "4.1.3",
    "vue-template-compiler": "2.7.14",
    "vue-template-loader": "1.1.0",
    "vuelidate": "0.7.7",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3",
    "webpack-merge": "4.2.2"
  }
}
