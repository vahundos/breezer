{
  "name": "breezer",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/vahundos/breezer.git",
  "scripts": {
    "dev-server": "webpack-dev-server --config webpack.dev.js",
    "test": "jest --verbose",
    "build": "webpack --config webpack.prod.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "modulePaths": [
      "<rootDir>/src/main/resources/static/js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/main/resources/static/js/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "axios": "^0.19.1",
    "vue": "^2.6.11",
    "vuetify": "^2.2.4",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "babel-bridge": "1.12.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "25.4.0",
    "babel-loader": "8.1.0",
    "css-loader": "3.4.2",
    "deepmerge": "4.2.2",
    "fibers": "5.0.0",
    "jest": "25.4.0",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "vue-jest": "3.0.5",
    "vue-loader": "15.9.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.11",
    "vue-template-loader": "1.1.0",
    "vuelidate": "0.7.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  }
}
